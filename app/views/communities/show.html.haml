- title "Community #{@community.name}"

%h1= "Leaderboard of the #{@community.name.camelcase} Community"

%table
  %thead
    %th User
    %th Green Miles
  - @community.users.by_green_miles.each do |user|
    %tr
      %td= user.username
      %td= user.green_miles

= link_to 'Show All Communities', communities_url
